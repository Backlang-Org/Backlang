{
    "name": "back",
    "displayName": "Backstention",
    "description": "Backlang For Backseat System",
    "icon": "media/logo.png",
    "version": "0.0.50",
    "publisher": "furesoft",
    "engines": {
        "vscode": "^1.66.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "back",
            "aliases": ["Backlang", "back"],
            "extensions": [".back"],
            "configuration": "./language-configuration.json",
            "icon": {
                "light": "./media/logo.svg",
                "dark": "./media/logo.svg"
            }
        }],
        "grammars": [{
            "language": "back",
            "scopeName": "source.back",
            "path": "./syntaxes/back.tmLanguage.json"
        }],
		"snippets": [
            {
                "language": "back",
                "path": "./snippets/snippets.code-snippets"
            }
        ],
        "commands": [
                {
                    "command": "back.updateTemplates",
                    "title": "Update Backlang Templates"
                }
            ],
        "main": "./out/extension.js",
        "activationEvents": ["onCommand:back.updateTemplates"],
        "menus": {
            "commandPalette": [
                    {
                        "command": "back.updateTemplates"
                    }
                ]
            },
        "devDependencies": {
            "@types/vscode": "^0.10.x",
            "typescript": "^1.6.2"
        },
        "scripts": {
            "vscode:prepublish": "npm run compile",
            "compile": "tsc -p ./"
	    }
    }
}