{
    "name": "back",
    "displayName": "Backstention",
    "description": "Backlang For Backseat System",
    "icon": "media/logo.png",
    "version": "0.0.43",
    "publisher": "furesoft",
    "engines": {
        "vscode": "^1.66.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "back",
            "aliases": ["Backlang", "back"],
            "extensions": [".back"],
            "configuration": "./language-configuration.json",
            "icon": {
                "light": "./media/logo.svg",
                "dark": "./media/logo.svg"
            }
        }],
        "grammars": [{
            "language": "back",
            "scopeName": "source.back",
            "path": "./syntaxes/back.tmLanguage.json"
        }],
		"snippets": [
            {
                "language": "back",
                "path": "./snippets/snippets.code-snippets"
            }
        ]
    }
}