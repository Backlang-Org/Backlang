const Math::PI = 3.1451;

fn Math::pow<T>(base : T, exponent : T)
    where T : u8..u32 
{
    let mut result = 1;

    for i in 0..exponent {
        result *= base;        
    }

    result;
}

fn Math::min<T>(x : u32, y : u32)
    where T : u8..u32 
{
    if x < y {
        x;
    }

    y;
}

fn Math::min<T>(x : T, y : T) 
    where T : u8..u32 
{
    if x > y {
        y;
    }

    x;
}